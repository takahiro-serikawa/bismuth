    "semanticHighlighting": true,
    "tokenColors": [
        { // コメント
            "scope": [
                "comment",
                "string.comment"   //?
                //comment.block.documentation.cs
            ],
            "settings": { "foreground": "#00ff00" }
        },
        { // コメント区切り文字
            "scope": [
                "punctuation.definition.comment",
                "punctuation.separator.annotation.result.python",
                "punctuation.separator.annotation.python"
            ],
            "settings": { "foreground": "#006000" }
        },
        { // <summary>
            "scope": [
                //"entity.name.tag.localname.cs",
                //"entity.name.tag.cs",
                "meta.tag.cs"
                //"entity.name.tag.html"
                //"meta.tag.structure.html.start.html"	//? <html>
            ],
            "settings": { "foreground": "#888888" }
        },
        {   // < ... >
            "scope": "punctuation.definition.tag",
            "settings": { "foreground": "#444444" }
        },
        {
            "scope": [
                "comment.line.double-slash.cs" // //
            ],
            "settings": {
                "foreground": "#00FF00"
            }
        },

        {   // 引用符 ' or "
            "scope": [
                "punctuation.definition.string",
                "punctuation.definition.char",
                "storage.type.string.python",   // f文字列 (python) f'...', r'...'
                "constant.character.format.placeholder.other.python",
                "punctuation.definition.interpolation", //.begin.cs $" {...} " (C#)
                "punctuation.definition.template-expression.begin.js",
                "punctuation.definition.template-expression.end.js"
            ],
            "settings": { "foreground": "#808080" }
        },
        {   // 文字列
            "scope": [
                "string.quoted", // " str ",
                "string.template"
            ],
            "settings": { "foreground": "#80ffff" }
        },
        {   // エスケープ \n
            "scope": "constant.character.escape",
            "settings": { "foreground": "#8080ff" }
        },
        {   // 書式
            "scope": "storage.type.format.python",
            "settings": { "foreground": "#997799" }
        },

        {   // キーワード
            "scope": [
                "keyword",
                "storage.modifier.cs", // public, const ... (C#)
                "storage.type.function", // function, def
                "storage.type.class", // class
                "storage.modifier.declaration.python",  // global
                "storage.type.js",
                "storage.type.built-in.primitive.cpp", //?
                "storage.type.primitive.cpp", //?
                "keyword.operator.new.cpp", // new
                "keyword.operator.delete.cpp", // delete
                "storage.type.built-in.primitive.cpp",
                "storage.type.primitive.cpp",
                "storage.type.modifier.access.control.private.cpp",
                "storage.type.modifier.access.control.public.cpp",
                "storage.type.*.cpp"
            ],
            "settings": {
                //"fontStyle": "bold",
                "foreground": "#909070"
            }
        },
        {
            "scope": [
                "keyword.control"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        { // キーワード　型名?
            "scope": "support.type.python",
            "settings": {
                //"fontStyle": "bold",
                "foreground": "#709090"
            }
        },
        { // 組み込み関数
            "scope": [
                //"support.function.builtin.python",    // print(), range()...
                "support.function.builtin"
            ],
            "settings": {
                "foreground": "#c0c0bb" // keywordより白っぽく
            }
        },

        {   // 定数
            "scope": "constant",
            "settings": { "foreground": "#FFFFC0" }
        },
        {   // 定数 識別子
            "scope": [
                "constant.other.caps.python",
                "entity.name.type.enum.cs", // enum (C#)
                "variable.other.constant.js"
            ],
            "settings": { "foreground": "#FFFFE0" }
        },
        { // 定数 組み込み
            "scope": [
                "variable.language.this.js",    // this
                //"constant.language.python",
                //"constant.language.null.cs",	// null, ...
                //"constant.language.boolean"	// true or false (C#)
                "constant.language" // null, true, false, undefined
            ],
            "settings": {
                "foreground": "#aaaa55" // キーワードと定数のあいのこ
            }
        },

        { // 演算子 記号
            "scope": "keyword.operator",
            "settings": { "foreground": "#ffcccc" }
        },
        {   // in とか not (python)
            "scope": [
                //"keyword.operator.expression.*.js",	// typeof
                //"keyword.operator.logical.python"
                "keyword.operator.expression"	// typeof
                //"keyword.operator.logical"		// in is and or
            ],
            "settings": { "foreground": "#aa8888" }
        },
        
        { // ユーザー型
            "scope": [
                "entity.name.type.class.cs", // (C#) class/struct 判別不能?
                "storage.type.cs"
                //"variable.other.object.cs" //???
            ],
            //"settings": { "foreground": "#A0E0C0" }
            "settings": { "foreground": "#c0ffe0" }
        },
        
        { // 普通の変数
            "scope": [
                "variable.other.object",
                "variable.other.property",
                "variable.other.readwrite",

                "entity.name.function.js"
            ],
            "settings": { "foreground": "#ffffff" }
        },
        { // 関数呼出時の引数名 func(arg=value, ...)
            "scope": "variable.parameter.function-call.python",
            "settings": { 
                //"foreground": "#e0d0ff"
                "foreground": "#cccccc"
                //"foreground": "#88AAAA"
            }
        },
        { // 関数呼び出し括弧 (...)
            "scope": [
                "punctuation.definition.arguments.begin.python",
                "punctuation.definition.arguments.end.python",
                "punctuation.definition.typeparameters.begin.cs", // List<...>
                "punctuation.definition.typeparameters.end.cs",
                "punctuation.parenthesis.open.cs", // func(...)
                "punctuation.parenthesis.close.cs",
                "punctuation.squarebracket.open.cs", // array[...]
                "punctuation.squarebracket.close.cs",
                "punctuation.section.angle-brackets.start.template.definition.cpp",
                "punctuation.section.angle-brackets.end.template.definition.cpp",
                "meta.brace.square.js",   // ( ... )
                "meta.brace.round.js",   // [ ... ]
                //"meta.brace.*.js"   // [ ... ] ( ... )
                "punctuation.separator.key-value.css",
                "punctuation.accessor.js"
            ],
            "settings": { "foreground": "#808080" }
        },
        {   // スライス : (python)
            "scope": "punctuation.separator.slice.python",
            //"settings": { "foreground": "#ffffff" }
            "settings": { "foreground": "#808080" }
        },

        // HTML
        { // <html>
            "scope": [
                "entity.name.tag" // html, xaml
                //"entity.name.tag.html"
                //"entity.name.tag.localname.xml"
            ],
            "settings": { "foreground": "#808060" }
        },
        { // <... name=... >
            "scope": [
                //"entity.other.attribute-name.xml",	// xml
                //"entity.other.attribute-name.localname.xml",
                //"entity.other.attribute-name",
                //x "meta.attribute.*.xml"
                //"meta.tag.preprocessor.xml"
                "entity.other.attribute-name.xml",
                "entity.other.attribute-name.html"
            ],
            "settings": { "foreground": "#88AAAA" }
        },
        {   // unit (CSS)
            "scope": [
                "keyword.other.unit"    // (CSS)
            ],
            "settings": {
                //"foreground": "#cc8888"
                "foreground": "#eecccc"
            }
        },
        {
            "scope": [
                //"entity.name.tag.css",    // selector { ... }
                "meta.selector.css"
            ],
            "settings": {
                //"foreground": "#CC9999"
                "foreground": "#dd8888"
            }
        },
        {
            "scope": [
                //"support.type.property-name.css",
                "meta.property-name.css",   // ... { prop: ... ; }
                //"meta.structure.dictionary.json.comments",
                //? "support.type.property-name.json.comments", // "name": ....
                //"meta.structure.dictionary.json.comments"
                "meta.object-literal.key.js"    //  { name: ... }
            ],
            "settings": {
                //"foreground": "#CCBBBB"
                //"foreground": "#e0d0ff"
                "foreground": "#ddc0c0"

            }
        },
        {
            "scope": "punctuation.separator.key-value.html",
            "settings": {
                "foreground": "#ffffff"
            }
        },

        // json
        {
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": { "foreground": "#aaaaaa" }
        },
        {
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": { "foreground": "#f8f8f8" }
        }
    ]
}